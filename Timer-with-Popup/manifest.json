{
  "manifest_version": 3,
  "name": "Focus-timer-Popup",
  "version": "1.0",
  "action": {
      "default_title": "timer-menu",
      "default_popup": "popup.html"
    },
  "permissions": ["tabs", "scripting", "alarms"],
  "host_permissions": ["https://*/*"],
  "background": {
    "service_worker": "background-wrapper.js"
  },
  "web_accessible_resources": [{
    "resources":["audio/t-rex-roar.mp3", "audio/yay-6120.mp3"],
    "matches":["https://*/*"]
}]
}